{
  "name": "webcomp-kg",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "npm run bundle && npm run copy-and-rename",
    "bundle": "ng build --prod --output-hashing none --single-bundle true",
    "rename": "node -e \"require('fs').rename('example/main-es2015.js', 'example/webcomp-kg.js', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "copy": "copyfiles --flat dist/webcomp-kg/main-es2015.js  example",
    "copy-and-rename": "npm run copy && npm run rename",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "ng lint",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/webcomp-kg/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/elements": "^9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@types/jquery": "^3.3.34",
    "@types/ol": "^6.2.0",
    "color": "^3.0.2",
    "colorbrewer": "^1.5.1",
    "colormap": "^2.3.1",
    "document-register-element": ">=1.7.2 <=1.8.1",
    "jquery": "^3.4.1",
    "ng-image-slider": "^2.4.1",
    "ngx-build-plus": "^9.0.6",
    "ol": "^6.2.2-dev.1585403261924",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.9.5",
    "codelyzer": "^5.2.2",
    "copyfiles": "^2.2.0",
    "http-server": "^0.12.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "protractor": "~5.4.3",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}
